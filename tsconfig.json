{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,
		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types/experimental"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"noEmit": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*", "worker-configuration.d.ts"]
		}
	}
}
